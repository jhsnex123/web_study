for(let i=0; i <10; i++){
	var start = Math.random();
	console.log(start);
	console.log(start*10);
	console.log(Math.floor(start*10));
	x = start;
	console.log(x);
}
for(let i=0; i <20; i++){
var end = Math.random();
console.log(end);
console.log(end*20);
console.log(Math.floor(end*20)+11);
y = end;
console.log(y);
}
sum=(x+y);
document.white(typeof sum);
</script>

